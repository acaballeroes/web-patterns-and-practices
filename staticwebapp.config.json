{
  "platform": {
    "apiRuntime": "node:22"
  },
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/_next/*", "/*.{css,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot,webp,json,xml,txt}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
