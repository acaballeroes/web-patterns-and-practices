{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "text/json",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["*.{css,scss,js,png,gif,jpg,svg,webp,ico,json,xml}"]
  }
}
